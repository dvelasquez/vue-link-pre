{
  "name": "vue-link-pre",
  "version": "1.0.2",
  "description": "Plugin for vuejs to utilize '<link rel=\"preload\"' and '<link rel=\"prefetch\"' functionality",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/index.d.ts",
    "src/*.d.ts"
  ],
  "scripts": {
    "build": "rollup -c=rollup.config.js",
    "watch": "rollup -w -c=rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvelasquez/vue-link-pre.git"
  },
  "keywords": [
    "preload",
    "prefetch",
    "dns-prefetch",
    "vuejs",
    "ssr"
  ],
  "author": "Danilo Velasquez<danilo.velasquez@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvelasquez/vue-link-pre/issues"
  },
  "homepage": "https://github.com/dvelasquez/vue-link-pre#readme",
  "devDependencies": {
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}
